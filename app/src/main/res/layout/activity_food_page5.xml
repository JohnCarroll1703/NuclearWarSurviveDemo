        app:layout_constraintTop_toBottomOf="@+id/textView46"
        app:srcCompat="@drawable/cucumber" />

    <ImageView
        android:id="@+id/imageView35"
        android:layout_width="179dp"
        android:layout_height="150dp"
        android:layout_marginTop="180dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.788"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView46"
        app:srcCompat="@drawable/snickers" />

    <TextView
        android:id="@+id/textView46"
        android:layout_width="400dp"
        android:layout_height="153dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="4dp"
        android:background="#4A4B2F"
        android:text="Консервированные фрукты и овощи. Срок хранения: около года.                                                                                                            Фрукты, овощи в запечатанном виде можно хранить целый год. После истечения данного срока, они будут съедобны, однако их вкусовые свойства и пищевая ценность может подпортиться. Как и в случае с мясными консервами, стои т следить за состоянием крышки (вздутие, провалы и т.п.). Также стоит обращать внимание на внешний вид продукции. "
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView45" />

    <TextView
        android:id="@+id/textView47"
        android:layout_width="401dp"
        android:layout_height="33dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="304dp"
        android:background="#4A4B2F"
        android:text="Сладости: печенье, шоколадные батончики, сгущенное молоко."
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView45" />

    <TextView
        android:id="@+id/textView48"
        android:layout_width="396dp"
        android:layout_height="39dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="488dp"
        android:background="#4A4B2F"
        android:text="Специи и травы - недооцененные продукты в списке необходимых для выживания."
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView45" />

    <TextView
        android:id="@+id/textView45"
        android:layout_width="318dp"
        android:layout_height="26dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="15dp"
        android:text="ЕДА В СЛУЧАЕ ЯДЕРНОЙ ВОЙНЫ."
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textView34"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/foodpage6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="108dp"
        android:text="след"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.956"
        app:layout_constraintStart_toEndOf="@+id/imageView36"
        app:layout_constraintTop_toBottomOf="@+id/textView48" />
</androidx.constraintlayout.widget.ConstraintLayout>