<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background4"
    tools:context=".EmergencyBackpack3">

    <ImageButton
        android:id="@+id/apoteke"
        android:layout_width="309dp"
        android:layout_height="59dp"
        android:layout_marginTop="228dp"
        android:contentDescription="food"
        android:scaleType="centerInside"
        android:soundEffectsEnabled="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.578"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView27"
        app:srcCompat="@drawable/apte4ka"
        android:onClick="startApotekePage"/>

    <TextView
        android:id="@+id/textView27"
        android:layout_width="326dp"
        android:layout_height="56dp"
        android:layout_marginTop="16dp"
        android:text="ПОДРОБНЕЕ ПРО МЕДИКАМЕНТЫ И ЕДУ"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/food"
        android:layout_width="309dp"
        android:layout_height="59dp"
        android:layout_marginTop="160dp"
        android:contentDescription="apoteke"
        android:scaleType="centerInside"
        android:soundEffectsEnabled="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.578"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView27"
        app:srcCompat="@drawable/foodicon"
        android:onClick="startFoodPage" />

</androidx.constraintlayout.widget.ConstraintLayout>